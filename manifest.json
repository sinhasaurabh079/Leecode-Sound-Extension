{
  "manifest_version": 3,
  "name": "LeetCode Sound Extension",
  "version": "1.0",
  "permissions": ["https://leetcode.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["content.js"]
    }
  ]
}
